# Google Gemini API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# Model Configuration
# OCR Model (vision-capable model for text extraction)
# Options: gemini-1.5-pro, gemini-1.5-flash, gemini-2.0-flash-exp, gemini-3-pro-preview
OCR_MODEL_NAME=gemini-3-pro-preview

# Structuring Model (for medical data structuring)
# Options: gemini-1.5-flash, gemini-1.5-pro, gemini-2.0-flash-exp, gemini-2.5-flash
STRUCTURING_MODEL_NAME=gemini-2.5-flash

# Optional Configuration
DEBUG=false
MAX_FILE_SIZE_MB=50
MAX_PAGE_COUNT=100
LOG_LEVEL=INFO
LOG_PHI=false

# OCR Configuration
OCR_CONFIDENCE_THRESHOLD=0.70
OCR_TIMEOUT_SECONDS=30

# Structuring Configuration
STRUCTURING_TIMEOUT_SECONDS=120
